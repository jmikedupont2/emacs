| 	#include <math.h>
| 	int
| 	main (int argc, char **argv)
| 	{
| 	  double d = argc;
| 	  float f = argc;
| 	  int i = argc;
| 	  long l = argc;
| 	  d = acos (d);
| 	  d = asin (d);
| 	  d = atan (d);
| 	  d = atan2 (d, d);
| 	  d = ceil (d);
| 	  d = copysign (d, d);
| 	  d = cos (d);
| 	  d = exp (d);
| 	  d = fabs (d);
| 	  d = floor (d);
| 	  d = fmod (d, d);
| 	  d = frexp (d, &i);
| 	  d = ldexp (d, i);
| 	  d = log (d);
| 	  d = log10 (d);
| 	  d = pow (d, d);
| 	  d = rint (d);
| 	  d = scalbn (d, l);
| 	  d = sin (d);
| 	  d = sqrt (d);
| 	  d = tan (d);
| 	  d = trunc (d);
| 	  f = fabsf (f);
| 	  f = powf (f, f);
| 	  i = ilogb (d);
| 	  i = signbit (d);
| 	  l = lrint (d);
| 	  l = lround (d);
| 	  return d == f && i == l;
| 	}
